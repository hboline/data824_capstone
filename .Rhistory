data <- read.csv("abalone.csv", header = TRUE)
data <- data %>%
mutate(sex = case_when(sex == "M" ~ "male",
sex == "F" ~ "female",
sex == "I" ~ "infant")) %>%
mutate(sex = factor(sex, levels = c("male", "female", "infant")),
across(.cols = colnames(.)[2:8], ~ .x*200),
age = rings + 1) %>%
filter(height < 60) %>%
select(-rings)
